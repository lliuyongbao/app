   <script src="../js/cookie.js"></script>
   <script>
	   debugger;
	   var str=unescape(location.href); //取得整个地址栏
	   var data = {};
	   if(str.indexOf("?") > -1){
		   var num=str.indexOf("?"); 
		   str=str.substr(num+1);
		   data = eval(str);
	   }
	   
   		$(".xinzen-btn").click(function(){
			if($(".xinzen-btnt-div").css("display")=="none"){
				$(".xinzen-btnt-div").show();
			}else{
				$(".xinzen-btnt-div").hide();
			}
		});
		
		$(function(){
			if(data != null){
				//目标价格
				$("#mblv").html(data.pricingResultKColl.loanTargetPrice);
				$("#mblvfdbl").html(data.pricingResultKColl.loanTargetPriceOfPencent);
				$("#mblvEVA").html(data.pricingResultKColl.targetPriceEva);

				//过程参数
				$("#zjcbl").html(data.pricingResultKColl.ftp_price);
				$("#yycbl").html(data.pricingResultKColl.operatingRate);
				$("#fxcbl").html(data.pricingResultKColl.expectedLossRate);
				$("#sscbl").html(data.pricingResultKColl.taxRate);
				$("#zbcbl").html(data.pricingResultKColl.capitalCostRate);
				$("#jjlrv").html(data.pricingResultKColl.targetRate);

				//价格调整
				$("#jgtzzj").html(data.pricingResultKColl.discount_total + "BP");

				//利率信息汇总
				$("#jzlvz").html(data.pricingResultKColl.baseLinePriceRateOfPercnet + "%");
				$("#jzEVA").html(data.pricingResultKColl.baseLinePriceRateEVA);

				$("#mblvz").html(data.pricingResultKColl.loanTargetPrice);
				$("#mbfdbl").html(data.pricingResultKColl.loanTargetPriceOfPencent);
				$("#mbEVA").html(data.pricingResultKColl.targetPriceEva);

				$("#jglvz").html(data.pricingResultKColl.discount_total + "BP");

				$("#yhlvz").html(data.pricingResultKColl.favoratePriceRate);
				$("#yhfdbl").html(data.pricingResultKColl.favoratePriceRateOfPercent);
				$("#yhfdbl").html(data.pricingResultKColl.favoratePricingRateEVA);

				$("#zxlvz").html(data.pricingResultKColl.favoratePriceRate);
				$("#zxfdbl").html(data.pricingResultKColl.favoratePriceRateOfPercent);
				$("#zxEVA").html(data.pricingResultKColl.favoratePricingRateEVA);
			}
		});
	</script>
